    "@types/react-dom": "18.2.4",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.1.3",
    "cypress": "^12.13.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier": "2.8.8",
    "storybook": "^7.0.18",
    "storybook-dark-mode": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
