    "@sentry/nextjs": "^7.53.1",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "next": "13.4.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.1",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.0.5",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.11",
    "yup": "^1.2.0"
  },
