    "@sentry/nextjs": "^7.27.0",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "13.1.6",
    "next-redux-wrapper": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.5",
    "react-hot-toast": "^2.4.0",
    "react-redux": "^8.0.5",
