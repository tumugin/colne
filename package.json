    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^12.0.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-plugin-storybook": "^0.6.7",
    "prettier": "2.8.4",
    "storybook-addon-next": "^1.7.1",
    "storybook-dark-mode": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
